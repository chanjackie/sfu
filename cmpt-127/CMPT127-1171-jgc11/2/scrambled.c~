int scrambled( unsigned int a[], unsigned int b[], unsigned int len ) {
	int i;
	for (i=0;i<len;i++) {
		int k;
		int flag1 = 0;
		int flag2 = 0;
		for (k=0;k<len;k++) {
			if (a[i] == b[k]) {
				flag1 = 1;
				break;
			}
		}
		for (k=0;k<len;k++) {
			if (b[i] == a[k]) {
				flag2 = 1;
				break;
			}
		}
		if (!flag1 || !flag2) {
			return 0;
		}
	}
	return 1;
}
